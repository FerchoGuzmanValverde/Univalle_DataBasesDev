<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
    <connectionStrings>
      <add name="HospitalAppConnectionString" connectionString="Data Source=localhost;Initial Catalog=HospitalUnivalle2019;Integrated Security=True" providerName="System.Data.SqlClient"/>
    </connectionStrings>
    <system.diagnostics>
      <trace autoflush="true" indentsize="4">
        <listeners>
          <add name="ListenerHospitalApp" type="System.Diagnostics.TextWriterTraceListener" initializeData="HopitalAppUnivalle.log"/>
        </listeners>
      </trace>
    </system.diagnostics>
</configuration>
